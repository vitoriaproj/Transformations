    <connection>frota</connection>
    <sql>-- Requisi&#xe7;&#xf5;es di&#xe1;rias&#xd;&#xa;&#x28;&#xd;&#xa;select&#xd;&#xa;  t_ite_req.cd_ite as cod_ite,&#xd;&#xa;  t_ite_req.dt_ite_req_atn as dat_saida,&#xd;&#xa;  t_ite_req.qt_ite_req_atn as qtd_saida,&#xd;&#xa;   &#x28;select top 1&#xd;&#xa;     t_cus_ite_dia.vl_cnt&#xd;&#xa;     from t_cus_ite_dia&#xd;&#xa;     where t_cus_ite_dia.dt_mov &#x3c;&#x3d; t_ite_req.dt_ite_req_atn and t_cus_ite_dia.cd_ite &#x3d; t_ite_req.cd_ite&#xd;&#xa;     order by t_cus_ite_dia.dt_mov desc&#xd;&#xa;     &#x29;  as val_ite,&#xd;&#xa;  t_ite_req.cd_pnt_apo as pt_apoio,&#xd;&#xa;  t_vei.numero as car_cod,&#xd;&#xa;  t_ite_req.cd_crt_cst as centroCusto,&#xd;&#xa;   0 as km_dia&#xd;&#xa;from t_ite_req&#xd;&#xa;left join t_vei on &#x28;t_ite_req.nr_vei &#x3d; t_vei.nr_vei&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;-- Complemento de &#xf3;leo&#xd;&#xa;&#x28;&#xd;&#xa;select &#xd;&#xa; t_cmp_oil_lub.cd_ite as cod_ite,&#xd;&#xa; t_cmp_oil_lub_exe.dt_cmp_oil_lub as dat_saida,&#xd;&#xa; t_cmp_oil_lub_exe.qt_cmp_oil_lub as qtd_saida,&#xd;&#xa; &#x28;select top 1&#xd;&#xa;t_cus_ite_dia.vl_cnt&#xd;&#xa;from t_cus_ite_dia&#xd;&#xa;where t_cus_ite_dia.dt_mov &#x3c;&#x3d; t_cmp_oil_lub_exe.dt_cmp_oil_lub and t_cus_ite_dia.cd_ite &#x3d; t_cmp_oil_lub.cd_ite&#xd;&#xa;order by t_cus_ite_dia.dt_mov desc&#xd;&#xa;&#x29;  as val_ite,&#xd;&#xa; t_cmp_oil_lub_exe.cd_pnt_apo as pt_apoio,&#xd;&#xa; t_vei.numero as car_cod,&#xd;&#xa; 0 as centroCusto,&#xd;&#xa; 0 as km_dia&#xd;&#xa;from t_cmp_oil_lub&#xd;&#xa;inner join t_cmp_oil_lub_exe &#xd;&#xa;  on &#x28;t_cmp_oil_lub.cd_cmp_oil_lub &#x3d; t_cmp_oil_lub_exe.cd_cmp_oil_lub&#x29; &#xd;&#xa;inner join t_vei &#xd;&#xa;  on &#x28;t_cmp_oil_lub_exe.nr_vei&#x3d;t_vei.nr_vei&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;--Retorno de renovadora&#xd;&#xa;&#x28;&#xd;&#xa;select &#xd;&#xa;  8042 as cod_ite,&#xd;&#xa;  t_ret_ren.dt_ret_ren as dat_saida,&#xd;&#xa;  1 as qtd_saida,&#xd;&#xa;  t_ret_ren_pne.vl_ren_pne as val_ite,&#xd;&#xa;  t_ret_ren.cd_pnt_apo as pt_apoio,&#xd;&#xa;  30000 as car_cod,&#xd;&#xa;  0 as centroCusto,&#xd;&#xa;  0 as km_dia&#xd;&#xa;from t_ret_ren_pne inner join t_ret_ren on t_ret_ren_pne.cd_ret_ren &#x3d; t_ret_ren.cd_ret_ren&#xd;&#xa;union all&#xd;&#xa;select &#xd;&#xa;  8042 as cod_ite,&#xd;&#xa;  t_ret_ren.dt_ret_ren as dat_saida,&#xd;&#xa;  t_ite_ret_ren.qt_ite as qtd_saida,&#xd;&#xa;  t_ite_ret_ren.vl_cus_ite as val_ite,&#xd;&#xa;  t_ite_ret_ren.cd_pnt_apo as pt_apoio,&#xd;&#xa;  30000 as car_cod,&#xd;&#xa;  0 as centroCusto,&#xd;&#xa;  0 as km_dia&#xd;&#xa;from t_ret_ren inner join t_ite_ret_ren on &#x28;t_ret_ren.cd_ret_ren &#x3d; t_ite_ret_ren.cd_ret_ren&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;-- Itens para recondicionamento&#xd;&#xa;&#x28;&#xd;&#xa;select&#xd;&#xa;	cd_ite as cod_ite,&#xd;&#xa;	dt_req as dat_saida,&#xd;&#xa;	qt_ite as qtd_saida,&#xd;&#xa;	&#x28;select top 1&#xd;&#xa;     t_cus_ite_dia.vl_cnt&#xd;&#xa;     from t_cus_ite_dia&#xd;&#xa;     where t_cus_ite_dia.dt_mov &#x3c;&#x3d; t_ite_rec.dt_req and t_cus_ite_dia.cd_ite &#x3d; t_ite_rec.cd_ite&#xd;&#xa;     order by t_cus_ite_dia.dt_mov desc&#xd;&#xa;     &#x29;  as val_ite,&#xd;&#xa;	cd_pnt_apo as pt_apoio,&#xd;&#xa;	null as car_cod,&#xd;&#xa;	0 as centroCusto,&#xd;&#xa;	0 as km_dia&#xd;&#xa;from t_ite_rec&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;--SCRIPT ABASTECIMENTO&#xd;&#xa;&#x28;&#xd;&#xa;SELECT&#xd;&#xa;	  1602 as cod_ite&#xd;&#xa;	, T_ABS_BVE.DT_ABS_BVE as dat_saida&#xd;&#xa;	, T_ABS_BVE.QT_ABS_BVE as qdt_saida&#xd;&#xa;	,&#x28;select top 1&#xd;&#xa;     t_cus_ite_dia.vl_cnt&#xd;&#xa;     from t_cus_ite_dia&#xd;&#xa;     where t_cus_ite_dia.dt_mov &#x3c;&#x3d; T_ABS_BVE.DT_ABS_BVE and t_cus_ite_dia.cd_ite &#x3d; 1602&#xd;&#xa;     order by t_cus_ite_dia.dt_mov desc&#xd;&#xa;     &#x29;  as val_ite&#xd;&#xa;	, 1 as pt_apoio&#xd;&#xa;	, t_vei.numero as car_cod&#xd;&#xa;	,0 as centroCusto&#xd;&#xa;	, &#x28;qt_abs_bve_km-qt_abs_bve_km_ant&#x29; as km_dia&#xd;&#xa;&#xd;&#xa;FROM T_ABS_BVE&#xd;&#xa;join t_vei on &#x28;t_vei.nr_vei&#x3d;t_abs_bve.nr_vei&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;&#x28;&#xd;&#xa;SELECT&#xd;&#xa;   T_ITE_AJT.CD_ITE AS COD_ITE -- Consulta de itens ajustados&#xd;&#xa;  ,T_AJT.DT_AJT AS DAT_SAIDA&#xd;&#xa;  ,T_ITE_AJT.QT_ITE_AJT AS QTD_SAIDA&#xd;&#xa;  ,&#x28;select top 1 --SUB SELECT PARA CONSIDERAR VALOR CONT&#xc1;BIL DE SAIDA DO ITEM NA DATA DA MVMT.&#xd;&#xa;     t_cus_ite_dia.vl_cnt&#xd;&#xa;     from t_cus_ite_dia&#xd;&#xa;     where t_cus_ite_dia.dt_mov &#x3c;&#x3d; T_AJT.DT_AJT and t_cus_ite_dia.cd_ite &#x3d; T_ITE_AJT.CD_ITE&#xd;&#xa;     order by t_cus_ite_dia.dt_mov desc&#xd;&#xa;     &#x29;  as val_ite&#xd;&#xa;  , 1 AS PT_APOIO&#xd;&#xa;  , NULL AS CAR_COD&#xd;&#xa;  ,0 as centroCusto&#xd;&#xa;  , 0 AS KM_DIA&#xd;&#xa;FROM T_ITE_AJT --TABELA DE ITENS AJUSTADOS&#x28;INCREMENTADOS OU DECREMENTADOS&#x29;&#xd;&#xa;INNER JOIN T_AJT ON &#x28;T_ITE_AJT.CD_AJT &#x3d; T_AJT.CD_AJT&#x29;&#xd;&#xa;WHERE T_ITE_AJT.FL_OPR &#x3d; 1&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;union all &#xd;&#xa;&#xd;&#xa;&#x28;&#xd;&#xa;SELECT &#xd;&#xa;  1602 as cod_ite -- c&#xf3;digo do diesel. &#xda;nico c&#xf3;digo at&#xe9; ent&#xe3;o.&#xd;&#xa; ,T_ABS_BVE.DT_ABS_BVE as dat_saida --Data do abastecimento &#xd;&#xa; ,T_ABS_AVL.QT_DIF  as qtd_saida --Quantidade &#xd;&#xa; ,&#x28;select top 1 --SUB SELECT PARA CONSIDERAR VALOR CONT&#xc1;BIL DE SAIDA DO ITEM NA DATA DA MVMT.&#xd;&#xa;     t_cus_ite_dia.vl_cnt&#xd;&#xa;     from t_cus_ite_dia&#xd;&#xa;     where t_cus_ite_dia.dt_mov &#x3c;&#x3d; T_ABS_BVE.DT_ABS_BVE and t_cus_ite_dia.cd_ite &#x3d; 1602&#xd;&#xa;     order by t_cus_ite_dia.dt_mov desc&#xd;&#xa;     &#x29;  as val_ite&#xd;&#xa; , 1 as pt_apoio&#xd;&#xa; , null as car_cod&#xd;&#xa; ,0 as centroCusto&#xd;&#xa; , 0 as km_dia --Coluna para concatenar a km mensal &#xd;&#xa;FROM T_ABS_AVL&#xd;&#xa;JOIN T_ABS_BVE ON &#x28;T_ABS_AVL.CD_ABS&#x3d;T_ABS_BVE.CD_ABS&#x29;&#xd;&#xa;GROUP BY T_ABS_BVE.CD_ABS , T_ABS_AVL.QT_DIF, T_ABS_BVE.DT_ABS_BVE&#xd;&#xa;&#x29;&#xd;&#xa;union all&#xd;&#xa;&#x28;&#xd;&#xa;&#x28;&#xd;&#xa;SELECT&#xd;&#xa;    &#x28;T_VEI.CD_CRT_CST &#x2a; -1&#x29; AS COD_ITE,&#xd;&#xa;    T_SER_TER.DT_SER_TER as dat_saida,&#xd;&#xa;    T_SER_TER_SER.QT_SER as qtd_saida,&#xd;&#xa;    &#x28;T_SER_TER_SER.VL_CUS_SER&#x2f;T_SER_TER_SER.QT_SER&#x29; as val_ite,&#xd;&#xa;    T_SER_TER_SER.CD_PNT_APO as pt_apoio,&#xd;&#xa;    T_VEI.NUMERO as car_cod,&#xd;&#xa;	0 as centroCusto,&#xd;&#xa;    0 as km_dia&#xd;&#xa;&#xd;&#xa;FROM T_SER_TER&#xd;&#xa;&#xd;&#xa;JOIN T_SER_TER_SER ON &#x28; T_SER_TER.CD_SER_TER &#x3d; T_SER_TER_SER.CD_SER_TER &#x29;&#xd;&#xa;JOIN T_VEI ON &#x28; T_SER_TER_SER.NR_VEI &#x3d; T_VEI.NR_VEI &#x29;&#xd;&#xa;where &#x28;T_SER_TER_SER.CD_PNT_APO&#x21;&#x3d;2&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;&#x28;&#xd;&#xa;SELECT&#xd;&#xa;    &#x28;T_SER_TER_SER_CST.cd_crt_cst &#x2a; -1&#x29; AS COD_ITE,&#xd;&#xa;    T_SER_TER.DT_SER_TER AS dat_saida,&#xd;&#xa;    T_SER_TER_SER_CST.QT_SER AS qtd_saida,  &#xd;&#xa;    &#x28;T_SER_TER_SER_CST.VL_CUS_SER&#x2f;T_SER_TER_SER_CST.QT_SER&#x29; as val_ite,&#xd;&#xa;    T_SER_TER.cd_pnt_apo as pt_apoio,&#xd;&#xa;    &#x28;T_SER_TER_SER_CST.cd_crt_cst &#x2a; -1&#x29; as car_cod,&#xd;&#xa;	0 as centroCusto,  &#xd;&#xa;    0 as km_dia&#xd;&#xa;&#xd;&#xa;FROM T_SER_TER_SER_CST&#xd;&#xa;&#xd;&#xa;join T_SER_TER ON &#x28;T_SER_TER_SER_CST.CD_SER_TER&#x3d;T_SER_TER.CD_SER_TER&#x29;&#xd;&#xa;where &#x28;T_SER_TER.cd_pnt_apo&#x21;&#x3d;2&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;order by 2 desc</sql>
