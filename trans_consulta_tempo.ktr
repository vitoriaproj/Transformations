    <connection>ordemservico</connection>
    <sql>&#xd;&#xa;select &#xd;&#xa;OS.osr_seq_ordem_servico,&#xd;&#xa;&#x28;SELECT &#xd;&#xa;	case when count&#x28;funcREL.osr_seq_ordem_servico&#x29;&#x3d;1&#xd;&#xa;		then substring&#x28;max&#x28;func.fun_dsc_nome&#x29;from 0 for 15&#x29;&#x7c;&#x7c;&#x27;.&#x27;&#xd;&#xa;		else substring&#x28;max&#x28;func.fun_dsc_nome&#x29;from 0 for 12&#x29;&#x7c;&#x7c;&#x27;.&#x2f;&#x27;&#x7c;&#x7c;substring&#x28;min&#x28;func.fun_dsc_nome&#x29;from 0 for 12&#x29;&#x7c;&#x7c;&#x27;...&#x27;&#xd;&#xa;	end as funcionario&#xd;&#xa;from fun_funcionario func&#xd;&#xa;join osf_ordem_servico_func funcREL using&#x28;fun_seq_funcionario&#x29;&#xd;&#xa;join osr_ordem_servico OSF using&#x28;osr_seq_ordem_servico&#x29;&#xd;&#xa;&#xd;&#xa;where OSF.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29; as funcionario,&#xd;&#xa;date_trunc&#x28;&#x27;seconds&#x27;, OS.osr_dat_inicio&#x29;,&#xd;&#xa;OS.osr_dat_fim,&#xd;&#xa;&#x28;SELECT count&#x28;tempoIMPED.osr_seq_ordem_servico&#x29; from imp_impedimento tempoIMPED WHERE tempoIMPED.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29; as qtdIMPED,&#xd;&#xa;cast&#x28;&#xd;&#xa;     case &#xd;&#xa;	when &#x28;SELECT count&#x28;tempoIMPED.osr_seq_ordem_servico&#x29; from imp_impedimento tempoIMPED WHERE tempoIMPED.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29;&#x3e;1 and &#x28;select count&#x28;relFUNC4.osr_seq_ordem_servico&#x29; from osf_ordem_servico_func relFUNC4 where relFUNC4.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico limit 1&#x29;&#x3e;1&#xd;&#xa;	then &#x28;sum&#x28;tempoMERC.det_num_tempo&#x29;&#x2f;&#x28;&#x28;&#x28;SELECT count&#x28;tempoIMPED.osr_seq_ordem_servico&#x29; from imp_impedimento tempoIMPED WHERE tempoIMPED.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29;&#x29;&#x2b;&#x28;select count&#x28;relFUNC4.osr_seq_ordem_servico&#x29; from osf_ordem_servico_func relFUNC4 where relFUNC4.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico limit 1&#x29;&#x29;&#x29;&#xd;&#xa;	when &#x28;SELECT count&#x28;tempoIMPED.osr_seq_ordem_servico&#x29; from imp_impedimento tempoIMPED WHERE tempoIMPED.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29;&#x3e;&#x3d;0 and &#x28;select count&#x28;relFUNC4.osr_seq_ordem_servico&#x29; from osf_ordem_servico_func relFUNC4 where relFUNC4.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico limit 1&#x29;&#x3e;1&#xd;&#xa;	then &#x28;sum&#x28;tempoMERC.det_num_tempo&#x29;&#x2f;&#x28;&#x28;&#x28;SELECT count&#x28;tempoIMPED.osr_seq_ordem_servico&#x29; from imp_impedimento tempoIMPED WHERE tempoIMPED.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29;&#x29;&#x2b;&#x28;select count&#x28;relFUNC4.osr_seq_ordem_servico&#x29; from osf_ordem_servico_func relFUNC4 where relFUNC4.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico limit 1&#x29;&#x29;&#x29;	&#xd;&#xa;	when &#x28;SELECT count&#x28;tempoIMPED.osr_seq_ordem_servico&#x29; from imp_impedimento tempoIMPED WHERE tempoIMPED.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29;&#x3e;1 and &#x28;select count&#x28;relFUNC4.osr_seq_ordem_servico&#x29; from osf_ordem_servico_func relFUNC4 where relFUNC4.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico limit 1&#x29;&#x3e;&#x3d;0&#xd;&#xa;	then &#x28;sum&#x28;tempoMERC.det_num_tempo&#x29;&#x2f;&#x28;&#x28;&#x28;SELECT count&#x28;tempoIMPED.osr_seq_ordem_servico&#x29; from imp_impedimento tempoIMPED WHERE tempoIMPED.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29;&#x29;&#x2b;&#x28;select count&#x28;relFUNC4.osr_seq_ordem_servico&#x29; from osf_ordem_servico_func relFUNC4 where relFUNC4.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico limit 1&#x29;&#x29;&#x29;	&#xd;&#xa;	else sum&#x28;tempoMERC.det_num_tempo&#x29;&#xd;&#xa;     end &#xd;&#xa;as numeric&#x28;19,2&#x29;&#x29;  as tmp_previsto,&#xd;&#xa;cast&#x28;&#xd;&#xa;     case &#xd;&#xa;	when &#x28;SELECT count&#x28;tempoIMPED.osr_seq_ordem_servico&#x29; from imp_impedimento tempoIMPED WHERE tempoIMPED.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29;&#x3e;0&#xd;&#xa;	then &#x28;&#x28;select sum&#x28;cast&#x28;extract&#x28;epoch from timp.imp_dat_fim-timp.imp_dat_inicio&#x29;&#x2f;3600 as numeric&#x28;19,5&#x29;&#x29;&#x29; from imp_impedimento timp where timp.osr_seq_ordem_servico&#x3d;os.osr_seq_ordem_servico&#x29;&#x29;&#xd;&#xa;	else 0&#xd;&#xa;     end &#xd;&#xa;as numeric&#x28;19,2&#x29;&#x29; as tmp_impedimento,&#xd;&#xa;case&#xd;&#xa;	when OS.ors_sta_status&#x3d;&#x27;FINALIZADA&#x27;&#xd;&#xa;        then cast&#x28;extract&#x28;epoch from OS.osr_dat_fim-OS.osr_dat_inicio&#x29;&#x2f;3600 as numeric&#x28;19,2&#x29;&#x29;&#xd;&#xa;        when OS.ors_sta_status&#x3d;&#x27;EXECUCAO&#x27;&#xd;&#xa;        then cast&#x28;&#x28;extract&#x28;epoch from now&#x28;&#x29;-OS.osr_dat_inicio&#x29;&#x2f;3600&#x29;-1 as numeric&#x28;19,2&#x29;&#x29;&#xd;&#xa;end  as tmp_execucao,&#xd;&#xa;&#x28;SELECT &#xd;&#xa;	gpMAFRO.grp_dsc_descricao&#xd;&#xa;&#xd;&#xa;from osr_ordem_servico OS1&#xd;&#xa;join ios_item_ordem_servico itemOS using&#x28;osr_seq_ordem_servico&#x29;&#xd;&#xa;join cht_chassis_det_merc tempoMERC using &#x28;cht_seq&#x29; &#xd;&#xa;join grm_grupo_mercedes gpMerc using&#x28;grm_seq_grupo_mercedes&#x29;&#xd;&#xa;join gsg_grupo_subgrupo relSUBGP on &#x28;gpMERC.grm_seq_grupo_mercedes&#x3d;cast&#x28;relSUBGP.gsg_cod_mercedes as integer&#x29;&#x29;&#xd;&#xa;join grp_grupo gpMAFRO on &#x28;relSUBGP.grp_seq_grupo&#x3d;gpMAFRO.grp_seq_grupo&#x29;&#xd;&#xa;&#xd;&#xa;where os1.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#xd;&#xa;limit 1&#x29; as gpMAFRO&#xd;&#xa;      &#xd;&#xa;&#xd;&#xa;FROM osr_ordem_servico  OS&#xd;&#xa;join osf_ordem_servico_func relFUNC on &#x28;os.osr_seq_ordem_servico&#x3d;relFUNC.osr_seq_ordem_servico&#x29;&#xd;&#xa;join fun_funcionario FUNC on &#x28;relFUNC.fun_seq_funcionario&#x3d;FUNC.fun_seq_funcionario&#x29;&#xd;&#xa;left join imp_impedimento tempoIMPED on&#x28;tempoIMPED.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29;&#xd;&#xa;join ios_item_ordem_servico itemOS on&#x28;itemOS.osr_seq_ordem_servico&#x3d;OS.osr_seq_ordem_servico&#x29;&#xd;&#xa;join cht_chassis_det_merc tempoMERC on&#x28;itemOS.cht_seq&#x3d;tempoMERC.cht_seq&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;where os.ors_sta_status&#x3d;&#x27;EXECUCAO&#x27; --or os.ors_sta_status&#x3d;&#x27;IMPEDIDA&#x27;&#xd;&#xa;&#xd;&#xa;group by 1,2,3,4,8&#xd;&#xa;order by 1,2,3,4 </sql>
    <limit>0</limit>
